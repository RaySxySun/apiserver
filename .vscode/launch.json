    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "APP_SETTINGS": "config.DevelopmentConfig",
                "DATABASE_URL":"postgresql+psycopg2://postgres:postgres@127.0.0.1:5432/apiserver_dev"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        }
    ]
}